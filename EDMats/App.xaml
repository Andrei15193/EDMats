<Application x:Class="EDMats.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="Application_Startup"
             xmlns:stores="clr-namespace:EDMats.Stores"
             xmlns:converters="clr-namespace:EDMats.Converters"
             xmlns:viewModels="clr-namespace:EDMats.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <stores:CommanderInfoStore x:Key="CommanderInfoStore" />
            <stores:GoalsStore x:Key="GoalsStore" />

            <viewModels:NotificationsViewModel x:Key="NotificationsViewModel" />

            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source=".\Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>